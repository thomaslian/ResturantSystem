<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lianreviews.resturantsystem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".category.CreateCategory"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".food.CreateFood"
            android:parentActivityName=".category.CreateCategory" />
        <activity
            android:name=".orders.CreateOrder"
            android:parentActivityName=".category.CreateCategory" />
        <activity
            android:name=".food.FoodPage"
            android:parentActivityName=".category.CreateCategory" />
        <activity
            android:name=".category.AddCategory"
            android:parentActivityName=".category.CreateCategory" />
        <activity
            android:name=".food.AddFoodName"
            android:parentActivityName=".category.CreateCategory" />
        <activity android:name=".orders.ViewOrder" />
        <activity android:name=".Archive"></activity>
    </application>

</manifest>